T_3.5 ;
    %vpi_call/w 3 75 "$display", "\0123. Test Read Back Written Address" {0 0 0};
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x600003339050_0, 0, 4;
    %wait E_0x600001430a80;
    %delay 1, 0;
