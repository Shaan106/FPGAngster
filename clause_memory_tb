    %flag_get/vec4 9;
    %jmp/0 T_1.6, 9;
    %load/vec4 v0x600003338e10_0;
    %load/vec4 v0x600003338c60_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_1.6;
    %flag_set/vec4 8;
